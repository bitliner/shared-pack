#!/usr/bin/env node

'use strict';

var SharedPack = require('../');
var Logger = new(require('grunt-legacy-log').Log)();
var path = require('path');

SharedPack.run({
	filename: path.resolve(__dirname, process.argv[2])
}, function(err) {
	if (err) {
		console.log('Error', err);
		return;
	}

	Logger.ok('Finished!');
});